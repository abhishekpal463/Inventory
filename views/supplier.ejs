<!doctype html>
<html>
    <head>
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">        </head>
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <body>
            <nav>
                <div class="nav-wrapper blue">
                  <a href="/" class="brand-logo" style="margin-left: 25px ;">Inventory</a>
                  <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li><a href="/materials">Materials</a></li>
                    <li class="active"><a href="/suppliers">Suppliers</a></li>
                  </ul>
                </div>
              </nav>
              <div class= "container"><br>
                <a href="/addsupplier" class="btn blue">add supplier</a>
                <table class="striped">
                  <%= typeof msg != 'undefined' ? msg : ''%>
                    <thead>
                      <tr>
                          <th>Companyname</th>
                          <th>Contact</th>
                          <th>Email Id</th>
                          <th>Created on</th>
                          <th></th>
                      </tr>
                    </thead>
                    <tbody>
                      <%for(let i=0;i<obj.length;i++){%>
                      <tr>
                        <td><%= obj[i].cmpname%></td>
                        <td><%= obj[i].contactno%></td>
                        <td><%= obj[i].emailid%></td>
                        <td><%= obj[i].created_on%></td>
                        <td>
                        <form action="/editsupplier/<%= obj[i]._id %>" method="get">
                           <input type="submit" class="green btn" value="edit">
                        </form>
                        </td>
                        <td>
                        <form action="/deletesupplier/<%= obj[i]._id %>" method="post">
                           <input type="submit" class="red btn" value="del">
                        </form>
                        </td>
                        <td>
                        <form action="/showsupplier/<%= obj[i]._id %>" method="get">
                           <input type="submit" class="blue btn" value="info">
                        </form>
                        </td>
                      </tr>
                      <% }%>
                    </tbody>
                  </table>
              </div>
                  <script
  src="https://code.jquery.com/jquery-3.3.1.min.js"
  integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
  crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>    </body>
</html>